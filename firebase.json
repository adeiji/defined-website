{
  "hosting": {
    "target": "definedcleaning",
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "predeploy": ["node scripts/deploy.js"],
    "postdeploy": ["node scripts/postdeploy.js"]
  }
}
